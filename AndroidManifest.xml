<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jannesion.koombeaapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.jannesion.koombeaapp.Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jannesion.koombeaapp.CreateUser"
            android:label="@string/title_activity_create_user" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jannesion.koombeaapp.Models"
            android:label="@string/title_activity_models" >
        </activity>
        <activity
            android:name="com.jannesion.koombeaapp.Favorite"
            android:label="@string/title_activity_favorite" >
        </activity>
        <activity
            android:name="com.jannesion.koombeaapp.DetailModel"
            android:label="@string/title_activity_detail_model" >
        </activity>
        <activity
            android:name="com.jannesion.koombeaapp.Galeria"
            android:label="@string/title_activity_galeria" >
        </activity>
    </application>

</manifest>
